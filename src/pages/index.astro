<style>
    
	html {
		background-color: #D5CAB9;
	}
	
	#eyes {
		animation: colorChange 5000ms infinite;
		fill: #D5CAB9;
	}

	@keyframes colorChange {
		0% {
		fill: #D5CAB9;
		}
		5% {
		fill: #0000FF;
		}
		10% {
		fill: #D5CAB9;
		}
		100% {
		fill: #D5CAB9;
		}
	}

	#leftEar {
		animation: rotateEar 5000ms infinite;
		animation-delay: 2000ms;
	}

	@keyframes rotateEar {
		0% {
		transform: rotate(0deg);
		}
		5% {
		transform: rotate(0.25deg);
		}
		10% {
		transform: rotate(0deg);
		}
		15% {
		transform: rotate(-0.25deg);
		}
		20%{
		transform: rotate(0deg);
		}
		100% {
		transform: rotate(0deg);
		}
	}
</style>